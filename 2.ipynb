    "def distributeCandies(candyType):\n",
    "    max_candies = len(candyType) // 2 \n",
    "    unique_candies = len(set(candyType))  \n",
    "    \n",
    "    return min(unique_candies, max_candies)\n",
    "candyType = [1,1,2,2,3,3]\n",
    "result = distributeCandies(candyType)\n",
    "print(result)\n"
   ]
